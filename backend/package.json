{
  "name": "studyconnect-backend",
  "version": "1.0.0",
  "description": "Backend API for StudyConnect",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "npm run test:unit && npm run test:bdd",
    "test:unit": "jest",
    "test:bdd": "cucumber-js --config ../cucumber.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:run": "node run-tests.js",
    "setup": "node complete-setup.js",
    "quick-test": "jest --passWithNoTests --silent"
  },
  "keywords": [
    "studyconnect",
    "task-management",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pg": "^8.15.5",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "supertest": "^7.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^5.2.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.6.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.0",
    "chai": "^6.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
